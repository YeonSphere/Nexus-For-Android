<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- TODO: Use <include> and <exclude> rules to define which data should be backed up -->
        <!-- <include ... /> -->
        <!-- <exclude ... /> -->
        <!-- Exclude specific shared preferences that contain sensitive information -->
        <exclude domain="sharedpref" path="sensitive_data.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- TODO: Use <include> and <exclude> rules to define which data should be transferred -->
        <!-- <include ... /> -->
        <!-- <exclude ... /> -->
        <!-- Include all shared preferences by default -->
        <include domain="sharedpref" path="."/>
    </device-transfer>
</data-extraction-rules>